#include <stdio.h>

void SumDob(double a, double b, double *sum, double *product) {
    *sum = a + b;
    *product = a * b;
}

int main() {
    int N;
    scanf("%d", &N);

    for (int i = 0; i < N; i++) {
        double a, b, sum, product;
        scanf("%lf %lf", &a, &b);
        
        SumDob(a, b, &sum, &product);

        printf("%.4lf %.4lf\n", sum, product);
    }

    return 0;}
