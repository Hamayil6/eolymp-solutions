#include <stdio.h>
#include <string.h>

int main()
{
    long long n;
    scanf("%lld", &n);
    long long ans = 0;
    char s[20];
    snprintf(s, sizeof(s), "%lld", n);
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] >= '0' && s[i] <= '9') {
            ans++;
        }
    }
    printf("%lld\n", ans);

    return 0;
}
