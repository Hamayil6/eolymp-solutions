#include <stdio.h>
#include <string.h>
#include <stdlib.h>    
int main() {
    char s[1000];
    int i, n, k;
    gets(s);
    n = strlen(s);
    k = 0;
    for (i = 0; i < n; ++i){
        if(s[i] == 'A' || s[i] == 'E' || s[i] == 'I' || s[i] == 'O' || s[i] == 'U' || s[i] == 'Y'){
            k ++;
        }
    }
    printf("%d", k);
    return 0;
}
