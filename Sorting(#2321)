#include<stdio.h>
int i, j, n, c;
int main() {
    scanf("%d", &n);
    int a[n]; 
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);  
    }
    for (j = 0; j < n - 1; j++) { 
        for (i = 0; i < n - 1 - j; i++) {  
            if (a[i] > a[i + 1]) { 
                c = a[i];
                a[i] = a[i + 1];
                a[i + 1] = c;
            }
        }
    }
    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }
    return 0;
}
