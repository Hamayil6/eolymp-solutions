#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int times[n][3];
    for (int i = 0; i < n; i++) {
        scanf("%d %d %d", &times[i][0], &times[i][1], &times[i][2]);
    }
    for (int i = 0; i < n - 1; i++) {  
        for (int j = i + 1; j < n; j++) {
            if (times[i][0] > times[j][0] || 
                (times[i][0] == times[j][0] && times[i][1] > times[j][1]) || 
                (times[i][0] == times[j][0] && times[i][1] == times[j][1] && times[i][2] > times[j][2])) {
                int temp[3];
                temp[0] = times[i][0];
                temp[1] = times[i][1];
                temp[2] = times[i][2];
                
                times[i][0] = times[j][0];
                times[i][1] = times[j][1];
                times[i][2] = times[j][2];
                
                times[j][0] = temp[0];
                times[j][1] = temp[1];
                times[j][2] = temp[2];
            }
        }
    }
    for (int i = 0; i < n; i++) {
        printf("%d %d %d\n", times[i][0], times[i][1], times[i][2]);
    }
    
    return 0;
}
