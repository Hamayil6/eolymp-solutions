#include <stdio.h>
int main() {
    char sentence[1000];
    int wordCount = 0;
    int inWord = 0;    
    gets(sentence);
    for (int i = 0; sentence[i]; i++) {
        if ((sentence[i] >= 'a' && sentence[i] <= 'z') ||
            (sentence[i] >= 'A' && sentence[i] <= 'Z') ||
            (sentence[i] >= '0' && sentence[i] <= '9')) {
            if (!inWord) {
                wordCount++;
                inWord = 1;
            }
        } else {

            inWord = 0;
        }
    }
    printf("%d", wordCount);
    return 0;
}
