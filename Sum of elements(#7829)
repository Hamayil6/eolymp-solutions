#include <stdio.h>
int main() {
    int n;
    double sum = 0.0, num;
    scanf("%d", &n);
    if (n > 100) {
        printf("Error: n should be 100 or less.\n");
        return 1;
    }
    for (int i = 0; i < n; i++) {
        scanf("%lf", &num);
        sum += num;
    }
    printf("%.1f\n", sum);
    return 0;
}
