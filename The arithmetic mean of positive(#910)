#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    double sum = 0.0;
    int positive_count = 0;

    for (int i = 0; i < n; ++i) {
        double num;
        scanf("%lf", &num);

        if (num > 0) {
            sum += num;
            positive_count++;
        }
    }

    if (positive_count > 0) {
        double mean = sum / positive_count;
        printf("%.2lf\n", mean);
    } else {
        printf("Not Found\n");
    }

    return 0;
}
