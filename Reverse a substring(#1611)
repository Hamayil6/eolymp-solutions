#include<stdio.h>
#include<string.h>
int main(){
   char str[10001];
   char tmp;
   int i;
   int j;
   scanf("%s", str);
   scanf("%d", &i);
   scanf("%d", &j);
   i=i-1;
   j=j-1;
   int len = strlen(str);
   if (len< i || len<j){
      return 0;
   }
   while (i<j){
      tmp=str[i];
      str[i]=str[j];
      str[j]=tmp;
      i++;
      j--;
   }
   printf("%s", str);
}
