#include <stdio.h>
#include <limits.h>
int main() {
    int n;
    scanf("%d", &n);
    int min1 = INT_MAX, min2 = INT_MAX;
    for (int i = 0; i < n; i++) {
        int num;
        scanf("%d", &num);
        if (num < min1) {
            min2 = min1;
            min1 = num;
        } else if (num < min2 && num != min1) {
            min2 = num;
        }
    }
    printf("%d %d\n", min1, min2);
    return 0;
}
