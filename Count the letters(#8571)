#include <stdio.h>
#include <ctype.h>

int main() {
    char s[1000], ch;
    int count = 0;
    fgets(s, sizeof(s), stdin);
    scanf("%c", &ch);
    ch = tolower(ch);
    for (int i = 0; s[i]; i++) {
        if (tolower(s[i]) == ch) {
            count++;
        }
    }

    printf("%d\n", count);
    
    return 0;
}
