#include <stdio.h>

int main() {
    int n, m;
    scanf("%d", &n);
    int array1[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &array1[i]);
    }
    scanf("%d", &m);
    int array2[m];
    for (int i = 0; i < m; i++) {
        scanf("%d", &array2[i]);
    }
    int count = 0;
    for (int i = 0; i < n; i++) {
        int flag = 0;
        for (int j = 0; j < m; j++) {
            if (array1[i] == array2[j]) {
                flag = 1;
                break;
            }
        }
        if (flag == 0) {
            count++;
        }
        flag = 0;
    }
    printf("%d", count);
    printf("\n");
    for (int i = 0; i < n; i++) {
        int flag = 0;
        for (int j = 0; j < m; j++) {
            if (array1[i] == array2[j]) {
                flag = 1;
                break;
            }
        }
        if (flag == 0) {
            printf("%d ", array1[i]);
        }
        flag = 0;
    }

    return 0;
}
