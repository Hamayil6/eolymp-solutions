#include <stdio.h>
#include <string.h>

int main()
{
    char s[1000];
    fgets(s, sizeof(s), stdin);
    int first_space = -1, last_space = -1;
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] == ' ') {
            if (first_space == -1) first_space = i; 
            last_space = i; 
        }
    }
    if (first_space == -1)
        printf("-1\n");
    else
        printf("%d %d\n", first_space, last_space);
    return 0;
}
