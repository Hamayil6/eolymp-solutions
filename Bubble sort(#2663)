#include <stdio.h>

int main() {
    int len;
    scanf("%d", &len);
    int nums[len];
    for (int i = 0; i < len; i++) {
        scanf("%d", &nums[i]);
    }
    int count = 0;
    for (int i = 0; i < len; i++) {
        for (int j = 0; j < len - 1; j++) {
            if (nums[j] > nums[j + 1]) {
                int temp = nums[j];
                nums[j] = nums[j + 1];
                nums[j + 1] = temp;
                count++;
            }
        }
    }
    printf("%d", count);

    return 0;
}
