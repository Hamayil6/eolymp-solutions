#include <stdio.h>
#include <ctype.h>
int main() {
    char text[251];
    gets(text);
    int count = 0;
    int in_word = 0;
    for (int i = 0; text[i] != '\0'; i++) {
        if (isalpha(text[i])) {
            if (!in_word) {
                count++;
                in_word = 1;
            }
        } else {
            in_word = 0;
        }
    }
    printf("%d\n", count);    
    return 0;}
