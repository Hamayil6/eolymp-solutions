#include <stdio.h>


long long f91(long long n) {
    if (n <= 100) {
        return f91(f91(n + 11));
    } else {
        return n - 10;
    }
}

int main() {
    long long n;
    while (scanf("%lld", &n) != EOF) {
        long long result = f91(n);
        printf("%lld\n", result);
    }
    return 0;
}
