#include <stdio.h>

void tricky_sorting(int array[], int len);
int main() {
    
    int n, number;
    scanf("%d", &n);
    int nums[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    tricky_sorting(nums, n);
    for (int i = 0; i < n; i++) {
        printf("%d ", nums[i]);
    }
}

void tricky_sorting(int array[], int len) {
    for (int i = 0; i < len - 1; i++) {
        for (int j = 0; j < len - 1; j++) {
            if ((array[j] % 10) > (array[j + 1] % 10)) {
                int temp = array[j];
                array[j] = array[j + 1];
                array[j + 1] = temp;
            } else if ((array[j] % 10) == (array[j + 1] % 10)) {
                if (array[j] > array[j + 1]) {
                    int temp = array[j];
                    array[j] = array[j + 1];
                    array[j + 1] = temp;
                }
            }   
        }
    }
}
