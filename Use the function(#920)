#include <stdio.h>

float max(float a, float b) {
    return (a > b) ? a : b;
}

float min(float a, float b) {
    return (a < b) ? a : b;
}

int main() {
    float x, y, z;
    scanf("%f %f %f", &x, &y, &z);

    float result = min(max(x, y), min(max(y, z), x + y + z));

    printf("%.2f\n", result);

    return 0;
}
