#include <stdio.h>
int main() {
    int n, i;
    scanf("%d", &n);  
    int arr[n];
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);  
    }
    int max = arr[0];
    for (i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    int sum = 0;
    for (i = 0; i < n; i++) {
        if (arr[i] != max) {
            sum += arr[i];
        }
    }
    printf("%d\n", sum);  
    return 0;
}
